{
    "name": "react-emoji-virtual-list",
    "description": "Implemented emoji picker using virtual list, solved the problem that 'emoji-picker-react' is too slow to open",
    "version": "0.1.4",
    "author":"chenkuangkuang",
    "license":"MIT",
    "keywords": [ "Component","emoji","React", "emoji virtual list"],
    "homepage": "github.com/chenkuangkuang/react-emoji-virtual-list",
    "repository": { "type": "git", "url": "github.com/chenkuangkuang/react-emoji-virtual-list" },
    "files": ["dist"],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.36",
        "@types/react": "^18.2.12",
        "@types/react-dom": "^18.2.5",
        "node-sass": "^7.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "npm run clean && npm run build-ts && npm run build-css && npm run build-assets",
        "clean": "rimraf ./dist",
        "build-ts": "tsc -p tsconfig.build.json",
        "build-css": "node-sass ./src/App.css ./dist/App.css",
        "build-assets": "node copyFile"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@egjs/react-infinitegrid": "^4.10.0"
    }
}
